{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@context": [
        "src/context"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@mdx": [
        "src/utils/mdx/next-mdx-relations.config.js"
      ],
      "@stitches": [
        "stitches.config.js"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
